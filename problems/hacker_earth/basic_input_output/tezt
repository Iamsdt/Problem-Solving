#include <stdio.h>
#include <string.h>

int main(){

    char a[10000],b[10000];

    int a1,b1,i,j,k,cnt=0,t;

    scanf("%d",&t);

    for(k=0;k<t;k++){
        scanf("%s",a);
        scanf("%s",b);

        a1=strlen(a);
        b1=strlen(b);

        if(a1 <= b1){
            for(i=0;i<a1;i++){

                for(j=0;j<b1;j++){

                    if(a[i]==b[j]){
                        b[j]=0;
                        cnt++;
                        break;
                    }
                }
            }
        }else{
            for(i=0;i<b1;i++){
                for(j=0;j<a1;j++){
                    if(b[i]==a[j]){
                        a[j]=0;
                        cnt++;
                        break;
                    }
                }
            }
        }

    printf("%d\n",(a1-cnt)+(b1-cnt));
    cnt=0;
    }

    return 0;
}